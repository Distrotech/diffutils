#, c-format
msgid "missing operand after `%s'"
msgstr "‘%s’後缺少了參數"

#: src/cmp.c:273 src/diff.c:759 src/diff3.c:317 src/sdiff.c:566
#, c-format
msgid "extra operand `%s'"
msgstr "多餘的參數‘%s’"

#: src/cmp.c:481
#, c-format
